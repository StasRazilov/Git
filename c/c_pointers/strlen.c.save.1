#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <assert.h>
#define N 20
int size_String(char *str)
{
    int count=0;
    for(;str[count]!='\0';count++);
    assert(count);
    return count; 
}
 
int main()
{
    char string1[N];
  
    printf("enter string: ");
    fgets(string1,N,stdin);

    int size_str=size_String(string1);
    printf("strlen is: %d \n", size_str);
    return 0;
}
